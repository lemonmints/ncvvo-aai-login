<configuration>
  <configSections>
    <section name="Federation" type="dk.nita.saml20.config.ConfigurationReader, dk.nita.saml20"/>
    <section name="SAML20Federation" type="dk.nita.saml20.config.ConfigurationReader, dk.nita.saml20"/>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net"/>
  </configSections>
  <connectionStrings>
    <add name="MyDatabase" connectionString="Server=192.168.60.170,1433;Database=TST_NCVVO;User Id=tst-ncvvo;Password=Initial11!;Trusted_Connection=True;Integrated Security=False;MultipleActiveResultSets=true;TrustServerCertificate=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <log4net>
    <root>
      <level value="All"/>
      <appender-ref ref="LogFileAppender"/>
    </root>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="C:\logs\log.txt"/>
      <param name="AppendToFile" value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="10MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%-5p%d{yyyy-MM-dd hh:mm:ss}-%m%n"/>
      </layout>
    </appender>
  </log4net>
  <Federation xmlns="urn:dk.nita.saml20.configuration">
    <SigningCertificate findValue="CN=jurek" storeLocation="LocalMachine" storeName="My" x509FindType="FindBySubjectDistinguishedName" validonly="no"/>
    <!-- The list of allowed audiences must at least contain the id of the service provider. -->
    <AllowedAudienceUris>
      <Audience>https://aai-ncvvo-test.akd.hr</Audience>
    </AllowedAudienceUris>
  </Federation>
  <SAML20Federation xmlns="urn:dk.nita.saml20.configuration">
    <ShowError>true</ShowError>
    <ServiceProvider id="https://aai-ncvvo-test.akd.hr" server="https://7b32a09e90df.ngrok.app/">
      <ServiceEndpoint localpath="~/login.ashx" type="signon" redirectUrl="http://localhost:12771/home" index="0"/>
      <ServiceEndpoint localpath="~/logout.ashx" type="logout" redirectUrl="http://localhost:12771/logout" index="1"/>
      <ServiceEndpoint localpath="~/metadata.ashx" type="metadata"/>
      <md:ContactPerson contactType="administrative" xmlns:md="urn:oasis:names:tc:SAML:2.0:metadata">
        <md:Company>Demo</md:Company>
        <md:GivenName>Ivan</md:GivenName>
        <md:SurName>Horvat</md:SurName>
        <md:EmailAddress>email@demo.hr</md:EmailAddress>
        <md:TelephoneNumber>1111111</md:TelephoneNumber>
      </md:ContactPerson>
      <NameIDFormats all="true"/>
    </ServiceProvider>
    <RequestedAttributes>
      <!-- The attributes that the demonstration identity provider issues. -->
      <att name="urn:FirstName" isRequired="true"/>
      <att name="urn:LastName" isRequired="true"/>
      <att name="urn:Age"/>
    </RequestedAttributes>
    <IDPEndPoints metadata="C:\metadata\">
      <!--<add id="https://login.aaiedu.hr/ms/saml2/idp/metadata.php">-->
      <add id="https://fed-lab.aaiedu.hr/ms/saml2/idp/metadata.php">
        <!--       <add id="DemoIdPEntityId">-->
        <CertificateValidation>
          <add type="dk.nita.saml20.Specification.SelfIssuedCertificateSpecification, dk.nita.saml20"/>
        </CertificateValidation>
      </add>
    </IDPEndPoints>
  </SAML20Federation>
  <system.diagnostics>
    <trace autoflush="true"/>
    <sources>
      <source name="dk.nita.saml20" switchValue="Information">
        <listeners>
          <add name="trace"/>
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="trace" type="System.Diagnostics.XmlWriterTraceListener" initializeData="C:\logs\saml2.tracelog"/>
    </sharedListeners>
  </system.diagnostics>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.8" />
      </system.Web>
  -->
  <system.web>
    <!-- No config attribute for Secure -->
    <httpCookies requireSSL="true"/>
    <sessionState cookieSameSite="None"/>
    <compilation debug="true" targetFramework="4.8"/>
    <httpRuntime targetFramework="4.7.2"/>
  </system.web>
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0"/>
      <remove name="OPTIONSVerbHandler"/>
      <remove name="TRACEVerbHandler"/>
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0"/>
      <add name="NemLoginMetadata" verb="*" path="~/metadata.ashx" type="dk.nita.saml20.protocol.Saml20MetadataHandler, dk.nita.saml20"/>
      <add name="NemLoginLogin" verb="*" path="~/login.ashx" type="dk.nita.saml20.protocol.Saml20SignonHandler, dk.nita.saml20"/>
      <add name="NemLoginLogout" verb="*" path="~/logout.ashx" type="dk.nita.saml20.protocol.Saml20LogoutHandler, dk.nita.saml20"/>
    </handlers>
  </system.webServer>
